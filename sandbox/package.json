{
  "name": "graphicBuilder",
  "config": {
    "s3_bucket": "graphics.expressnews.com",
    "slug": "vue-sandbox"
  },
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "user/authorize": "./bin/authorize.js",
    "spreadsheet/fetch": "./bin/fetch_spreadsheet.js",
    "spreadsheet/edit": "./bin/edit_spreadsheet.js",
    "assets/push": "./bin/push_assets_to_s3.sh",
    "assets/pull": "./bin/pull_assets_from_s3.sh",
    "deploy": "./bin/deploy.sh && npm run assets/push"
  },
  "dependencies": {
    "express-news-styleguide": "^1.0.4",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "chalk": "^2.4.1",
    "eslint-plugin-only-warn": "^1.0.1",
    "googleapis": "^16.1.0",
    "inquirer": "^6.1.0",
    "marked": "^0.5.0",
    "nconf": "^0.10.0",
    "node-sass": "^4.9.0",
    "opn": "^5.3.0",
    "request": "^2.88.0",
    "sass-loader": "^7.0.1",
    "untildify": "^3.0.3",
    "vue-template-compiler": "^2.5.17",
    "xlsx": "^0.13.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "only-warn"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
